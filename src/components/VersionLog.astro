---
import { versionLogs } from "../config/versionLog";
---

<div>
  {versionLogs.map((log) => (
    <div class="mb-6">
      <h3 class="text-xl font-semibold mb-2">
        v{log.version}
        {log.date && (
          <span class="text-sm font-normal text-gray-600">
            ({log.date})
          </span>
        )}
      </h3>
      <ul class="list-decimal list-inside ml-4 space-y-1">
        {log.changes.map((change) => (
          <li>{change}</li>
        ))}
      </ul>
    </div>
  ))}
</div>

